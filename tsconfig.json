{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es6",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "dist",
      "baseUrl": "src",
      "paths": {
         "@core": [
            "api/core/*"
         ],
         "@classes": [
            "api/core/types/classes"
         ],
         "@classes/*": [
            "api/core/types/classes/*"
         ],
         "@config/*": [
            "api/config/*"
         ],
         "@controllers/*": [
            "api/core/controllers/*"
         ],
         "@crons/*": [
            "api/core/crons/*"
         ],
         "@decorators/*": [
            "api/core/types/decorators/*"
         ],
         "@enums": [
            "api/core/types/enums"
         ],
         "@enums/*": [
            "api/core/types/enums/*"
         ],
         "@errors": [
            "api/core/types/errors"
         ],
         "@errors/*": [
            "api/core/types/errors/*"
         ],
         "@events": [
            "api/core/types/events"
         ],
         "@events/*": [
            "api/core/types/events/*"
         ],
         "@factories/*": [
            "api/core/factories/*"
         ],
         "@interfaces": [
            "api/core/types/interfaces"
         ],
         "@interfaces/*": [
            "api/core/types/interfaces/*"
         ],
         "@middlewares/*": [
            "api/core/middlewares/*"
         ],
         "@models/*": [
            "api/core/models/*"
         ],
         "@public-content": [
            "../app.json"
         ],
         "@repositories/*": [
            "api/core/repositories/*"
         ],
         "@resources/*": [
            "api/resources/*"
         ],
         "@routes/*": [
            "api/core/routes/v1/*"
         ],
         "@router": [
            "api/core/routes/index.route.ts"
         ],
         "@schemas": [
            "api/core/types/schemas"
         ],
         "@schemas/*": [
            "api/core/types/schemas/*"
         ],
         "@services/*": [
            "api/core/services/*"
         ],
         "@shared/*": [
            "api/shared/*"
         ],
         "@seeders/*": [
            "api/core/seeders/*"
         ],
         "@types": [
            "api/core/types/types"
         ],
         "@types/*": [
            "api/core/types/types/*"
         ],
         "@utils/*": [
            "api/core/utils/*"
         ],
         "@validations/*": [
            "api/core/validations/*"
         ]
      },
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true
   }
}